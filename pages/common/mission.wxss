.mission {
  display: flex;
  padding: 16rpx 30rpx;
  border-bottom: 1px solid #e5e5e5;
}

.mission-hover {
  background-color: #d9d9d9;
}

.mission image {
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
}

.mission-info {
  margin-left: 26rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.mission-info>text:first-child {
  font-weight: bold;
}